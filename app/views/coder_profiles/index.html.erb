<h1>Listing coder_profiles</h1>

<table>
  <tr>
    <th>User</th>
    <th>Github profile</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coder_profiles.each do |coder_profile| %>
  <tr>
    <td><%= coder_profile.user_id %></td>
    <td><%= coder_profile.github_profile %></td>
    <td><%= link_to 'Show', coder_profile %></td>
    <td><%= link_to 'Edit', edit_coder_profile_path(coder_profile) %></td>
    <td><%= link_to 'Destroy', coder_profile, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

